'Тип: {pet[\"species\"]}')\n","    print(f'Возраст: {pet[\"age\"]}')\n","    print(f'Голод: {pet[\"hunger\"]}')\n","    print(f'Счастье: {pet[\"happiness\"]}')\n","    print(f'Энергия: {pet[\"energy\"]}')\n","\n","#Главная функция для управления интерфейсом\n","def main():\n","    functions = ['Усыновление нового питомца',\n","                 'Покормить питомца',\n","                 'Поиграть с питомцем',\n","                 'Уложить питомца спать',\n","                 'Показать состояние питомца',\n","                 'Закрыть симулятор'\n","                 ]\n","\n","    while True:\n","        print('\\nФункции симулятора виртуального питомца: ')\n","        for count, function in enumerate(functions, 1):\n","            print(f'{count}. {function}')\n","\n","        choice = int(input('\\nВыберите нужную вам функцию: '))\n","\n","        if choice == 1:\n","            adopt_pet()\n","\n","        elif choice == 2:\n","            if pets == []:\n","                print('\\nУ вас пока нет питомцев.')\n","            else:\n","                print('\\nСписок ваших питомцев по именам: ')\n","                for count, pet in enumerate(pets, 1):\n","                    print(f'{count}) {pet[\"name\"]}')\n","\n","                pet_index = int(input('\\nВыберите номер нужного вам питомца из предложенного списка: '))\n","\n","                if pet_index <= len(pets):\n","                    feed(pets[pet_index - 1])\n","                else:\n","                    print('\\nНекорректный номер питомца.')\n","\n","        elif choice == 3:\n","            if pets == []:\n","                print('\\nУ вас пока нет питомцев.')\n","            else:\n","                print('\\nСписок ваших питомцев по именам: ')\n","                for count, pet in enumerate(pets, 1):\n","                    print(f'{count}) {pet[\"name\"]}')\n","\n","                pet_index = int(input('\\nВыберите номер нужного вам питомца из предложенного списка: '))\n","\n","                if pet_index <= len(pets):\n","                    play(pets[pet_index - 1])\n","                else:\n","                    print('\\nНекорректный номер питомца.')\n","\n","        elif choice == 4:\n","            if pets == []:\n","                print('\\nУ вас пока нет питомцев.')\n","            else:\n","                print('\\nСписок ваших питомцев по именам: ')\n","                for count, pet in enumerate(pets, 1):\n","                    print(f'{count}) {pet[\"name\"]}')\n","\n","                pet_index = int(input('\\nВыберите номер нужного вам питомца из предложенного списка: '))\n","\n","                if pet_index <= len(pets):\n","                    sleep(pets[pet_index - 1])\n","                else:\n","                    print('\\nНекорректный номер питомца.')\n","\n","        elif choice == 5:\n","            if pets == []:\n","                print('\\nУ вас пока нет питомцев.')\n","            else:\n","                print('\\nСписок ваших питомцев по именам: ')\n","                for count, pet in enumerate(pets, 1):\n","                    print(f'{count}) {pet[\"name\"]}')\n","\n","                pet_index = int(input('\\nВыберите номер нужного вам питомца из предложенного списка: '))\n","\n","                if pet_index <= len(pets):\n","                    display_status(pets[pet_index - 1])\n","                else:\n","                    print('\\nНекорректный номер питомца.')\n","\n","        elif choice == 6:\n","            break\n","\n","        else:\n","            print('\\nНекорректный выбор, попробуйте еще раз.')\n","\n","main()\n","\n","\n"],"metadata":{"id":"hE7mF-HXpJLO","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1731410610691,"user_tz":-300,"elapsed":151674,"user":{"displayName":"Аноним Анонимов","userId":"15875348424582677757"}},"outputId":"ef2a3992-a2ac-4be1-9705-e177afd0a79e"},"execution_count":null,"outputs":[{"name":"stdout","output_type":"stream","